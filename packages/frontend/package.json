{
  "name": "@l2beat/frontend",
  "version": "0.1.0",
  "private": true,
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0 <2"
  },
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "build:dependencies": "cd ../.. && yarn build:dependencies",
    "clean": "rm -rf build",
    "format:fix": "biome format --write .",
    "format": "biome format .",
    "lint:fix": "biome check --formatter-enabled=false --apply .",
    "lint": "biome check --formatter-enabled=false .",
    "start": "yarn start:staging",
    "start:local": "DEPLOYMENT_ENV=local gulp watch",
    "start:mock": "DEPLOYMENT_ENV=local MOCK=true gulp watch",
    "start:staging": "DEPLOYMENT_ENV=staging gulp watch",
    "start:production": "DEPLOYMENT_ENV=production gulp watch",
    "test": "mocha",
    "tinify-logos": "node -r esbuild-register scripts/logos/tinifyLogos.ts",
    "generate-og": "node -r esbuild-register scripts/meta-images/generate.ts",
    "typecheck": "tsc --noEmit",
    "chromatic": "chromatic",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "(cd ../.. && wsrun -p @l2beat/{shared-pure,config} -recsm build --module es6) && storybook build"
  },
  "dependencies": {
    "@bugsnag/js": "^7.22.3",
    "@l2beat/config": "*",
    "@l2beat/shared-pure": "*",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.191",
    "@types/markdown-it": "^12.2.3",
    "@types/qrcode": "^1.4.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.1",
    "puppeteer": "^14.3.0",
    "qrcode": "^1.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwind-merge": "^2.2.1",
    "tinify": "^1.7.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1",
    "@storybook/addon-actions": "^8.1.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^8.1.1",
    "@storybook/react-vite": "^8.1.1",
    "@storybook/test": "^8.1.1",
    "autoprefixer": "^10.4.13",
    "chromatic": "^11.3.5",
    "del": "^6.1.1",
    "gulp": "^4.0.2",
    "http-proxy-middleware": "^2.0.6",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "storybook": "^8.1.1",
    "storybook-addon-pseudo-states": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.1",
    "vite": "^5.2.11",
    "vite-plugin-turbosnap": "^1.0.3"
  }
}
